{
	"name": "@reecem/clockworks",
	"version": "0.1.1",
	"description": "Webworker for managing timers and intervals",
	"source": "src/clockworks.js",
	"main": "dist/clockworks.js",
	"umd:main": "dist/clockworks.umd.js",
	"module": "dist/clockworks.module.js",
	"unpkg": "dist/clockworks.umd.js",
	"scripts": {
		"clean": "rm -rf ./dist",
