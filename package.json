	"license": "MIT",
	"bugs": {
        "url": "https://github.com/ReeceM/clockworks/issues"
	},
	"homepage": "https://github.com/ReeceM/clockworks#readme",
	"devDependencies": {
		"microbundle": "^0.12.0"
	}
}
